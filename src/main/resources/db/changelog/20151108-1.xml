<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <changeSet author="andras" id="20151108-1">
        <createTable tableName="shop_stock">
            <column name="id" type="int unsigned" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="shop_id" type="int unsigned">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="item_id" type="int unsigned">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="item_quantity" type="int unsigned">
                <constraints nullable="false" />
            </column>
           <column name="last_regeneration_date" type="datetime" defaultValue="1970-00-00 00:00:00">
                <constraints nullable="false" />
           </column>
        </createTable>
    </changeSet>
</databaseChangeLog>